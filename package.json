    "postcss-tape": "5.0.2",
    "pre-commit": "1.2.2",
    "rollup": "1.26.3",
    "rollup-plugin-babel": "4.3.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
